{
  "kind": "collectionType",
  "collectionName": "works",
  "info": {
    "singularName": "work",
    "pluralName": "works",
    "displayName": "Works",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "ext": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true
    },
    "color_bg": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color",
      "required": true
    },
    "color_element": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color",
      "required": true
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "project_summary": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "required": true,
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "project_details": {
      "displayName": "Project Details",
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "project-details.project-details"
    },
    "team_members": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "team-members.team-members"
    },
    "problem": {
      "displayName": "Problem",
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "problem-list.problem"
    },
    "solution": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    }
  }
}
